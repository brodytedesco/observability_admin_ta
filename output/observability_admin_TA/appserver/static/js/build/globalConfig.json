{
    "pages": {
        "configuration": {
            "tabs": [
                {
                    "name": "account",
                    "restHandlerModule": "observability_account_verification",
                    "restHandlerClass": "observability_account_validator",
                    "title": "Account",
                    "table": {
                        "actions": [
                            "edit",
                            "delete",
                            "clone"
                        ],
                        "header": [
                            {
                                "label": "Name",
                                "field": "name"
                            }
                        ]
                    },
                    "entity": [
                        {
                            "type": "text",
                            "label": "Name",
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Input Name must begin with a letter and consist exclusively of alphanumeric characters and underscores.",
                                    "pattern": "^[a-zA-Z]\\w*$"
                                },
                                {
                                    "type": "string",
                                    "errorMsg": "Length of input name should be between 1 and 100",
                                    "minLength": 1,
                                    "maxLength": 100
                                }
                            ],
                            "field": "name",
                            "help": "A unique name for the data input.",
                            "required": true
                        },
                        {
                            "type": "text",
                            "label": "Realm",
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Input Name must begin with a letter and consist exclusively of alphanumeric characters and underscores.",
                                    "pattern": "^[a-zA-Z]\\w*$"
                                },
                                {
                                    "type": "string",
                                    "errorMsg": "Length of input name should be between 1 and 5",
                                    "minLength": 1,
                                    "maxLength": 5
                                }
                            ],
                            "field": "realm",
                            "help": "The realm of your Observability stack (eg: au0, us2, eu0)",
                            "required": true
                        },
                        {
                            "type": "text",
                            "label": "Token",
                            "validators": [
                                {
                                    "type": "string",
                                    "errorMsg": "Length of input name should be between 20 and 25",
                                    "minLength": 20,
                                    "maxLength": 25
                                }
                            ],
                            "field": "token",
                            "help": "The API Token from Observability (User token if ingesting Tokens)",
                            "required": true,
                            "encrypted": true
                        }
                    ]
                },
                {
                    "name": "logging",
                    "title": "Logging",
                    "entity": [
                        {
                            "type": "singleSelect",
                            "label": "Log level",
                            "options": {
                                "disableSearch": true,
                                "autoCompleteFields": [
                                    {
                                        "value": "DEBUG",
                                        "label": "DEBUG"
                                    },
                                    {
                                        "value": "INFO",
                                        "label": "INFO"
                                    },
                                    {
                                        "value": "WARNING",
                                        "label": "WARNING"
                                    },
                                    {
                                        "value": "ERROR",
                                        "label": "ERROR"
                                    },
                                    {
                                        "value": "CRITICAL",
                                        "label": "CRITICAL"
                                    }
                                ]
                            },
                            "defaultValue": "INFO",
                            "field": "loglevel",
                            "required": true,
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Log level must be one of: DEBUG, INFO, WARNING, ERROR, CRITICAL",
                                    "pattern": "^DEBUG|INFO|WARNING|ERROR|CRITICAL$"
                                }
                            ]
                        }
                    ]
                }
            ],
            "title": "Configuration",
            "description": "Set up your add-on"
        },
        "inputs": {
            "services": [
                {
                    "name": "observability_object",
                    "entity": [
                        {
                            "type": "text",
                            "label": "Name",
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Input Name must begin with a letter and consist exclusively of alphanumeric characters and underscores.",
                                    "pattern": "^[a-zA-Z]\\w*$"
                                },
                                {
                                    "type": "string",
                                    "errorMsg": "Length of input name should be between 1 and 100",
                                    "minLength": 1,
                                    "maxLength": 100
                                }
                            ],
                            "field": "name",
                            "help": "A unique name for the data input.",
                            "required": true
                        },
                        {
                            "type": "singleSelect",
                            "label": "Observability Object Type",
                            "field": "object_type",
                            "options": {
                                "autoCompleteFields": [
                                    {
                                        "value": "alert",
                                        "label": "Alert"
                                    },
                                    {
                                        "value": "chart",
                                        "label": "Chart"
                                    },
                                    {
                                        "value": "dashboard",
                                        "label": "Dashboard"
                                    },
                                    {
                                        "value": "detector",
                                        "label": "Detector"
                                    },
                                    {
                                        "value": "synthetic",
                                        "label": "Synthetic"
                                    },
                                    {
                                        "value": "synthetic_detailed",
                                        "label": "Detailed Synthetic"
                                    },
                                    {
                                        "value": "token",
                                        "label": "Token"
                                    }
                                ]
                            },
                            "help": "The type of object to be ingested",
                            "required": true
                        },
                        {
                            "type": "singleSelect",
                            "label": "Account to use",
                            "options": {
                                "referenceName": "account"
                            },
                            "field": "account",
                            "required": true
                        },
                        {
                            "type": "text",
                            "field": "interval",
                            "label": "Interval",
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Interval must be either a non-negative number, CRON interval or -1.",
                                    "pattern": "^((?:-1|\\d+(?:\\.\\d+)?)|(([\\*\\d{1,2}\\,\\-\\/]+\\s){4}[\\*\\d{1,2}\\,\\-\\/]+))$"
                                },
                                {
                                    "type": "number",
                                    "range": [
                                        10,
                                        86400
                                    ],
                                    "errorMsg": "Interval must be between 10 and 86400"
                                }
                            ],
                            "help": "Time interval of the data input, in seconds.",
                            "required": true,
                            "defaultValue": "300"
                        },
                        {
                            "type": "singleSelect",
                            "field": "index",
                            "label": "Index",
                            "defaultValue": "observability_admin",
                            "options": {
                                "endpointUrl": "data/indexes?search=isInternal=0+disabled=0",
                                "denyList": "^_.*$",
                                "createSearchChoice": true
                            },
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Index names must begin with a letter or a number and must contain only letters, numbers, underscores or hyphens.",
                                    "pattern": "^[a-zA-Z0-9][a-zA-Z0-9\\\\_\\\\-]*$"
                                },
                                {
                                    "type": "string",
                                    "errorMsg": "Length of index name should be between 1 and 80.",
                                    "minLength": 1,
                                    "maxLength": 80
                                }
                            ]
                        }
                    ],
                    "inputHelperModule": "observability_object_helper",
                    "title": "observability_object"
                }
            ],
            "title": "Inputs",
            "description": "The Splunk Observability API has a return limit of 10,000 for all objects, therefore if you have more then 10,000 of a object, some data will not be included (charts are a main culprit for this)",
            "subDescription": {
                "text": "Alerts: Ingests all active alerts\n Charts: Ingests all charts \n Dashboards: Ingests all Dashboards \n Detectors: Ingest all Detectors \n Synthetics: Ingest all Synthetics \n Detailed Synthetics: Ingests all Synthetics, with a high level of detail. WARNING: This takes a long time, as detailed information is queried one by one. \n Tokens: Ingests all Tokens. You must use a account with a 'User API Token', refer to set-up"
            },
            "table": {
                "actions": [
                    "edit",
                    "delete",
                    "clone"
                ],
                "header": [
                    {
                        "label": "Name",
                        "field": "name"
                    },
                    {
                        "label": "Interval",
                        "field": "interval"
                    },
                    {
                        "label": "Index",
                        "field": "index"
                    },
                    {
                        "label": "Object Type",
                        "field": "object_type"
                    },
                    {
                        "label": "Status",
                        "field": "disabled"
                    }
                ],
                "moreInfo": [
                    {
                        "label": "Name",
                        "field": "name"
                    },
                    {
                        "label": "Interval",
                        "field": "interval"
                    },
                    {
                        "label": "Index",
                        "field": "index"
                    },
                    {
                        "label": "Object Type",
                        "field": "object_type"
                    },
                    {
                        "label": "Status",
                        "field": "disabled",
                        "mapping": {
                            "true": "Inactive",
                            "false": "Active"
                        }
                    }
                ]
            }
        },
        "dashboard": {
            "panels": [
                {
                    "name": "default"
                }
            ]
        }
    },
    "meta": {
        "name": "observability_admin_TA",
        "restRoot": "observability_admin_TA",
        "version": "1.2.0",
        "displayName": "O11y (Splunk Observability) TA",
        "schemaVersion": "0.0.9",
        "supportedThemes": [
            "light",
            "dark"
        ],
        "_uccVersion": "5.55.0"
    }
}
